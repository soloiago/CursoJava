<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory name="SessionFactory">
        <property name="hibernate.connection.driver_class">oracle.jdbc.driver.OracleDriver</property>
        <property name="hibernate.connection.password">iago</property>
        <property name="hibernate.connection.url">jdbc:oracle:thin:@192.168.1.120:1521:XE</property>
        <property name="hibernate.connection.username">iago</property>
        <property name="hibernate.dialect">org.hibernate.dialect.Oracle10gDialect</property>
        <property name="hibernate.show.sql">true</property> 
        
        <mapping class="com.iago.springapp.domain.Stock"></mapping>
        <mapping class="com.iago.springapp.domain.pojo.Comments"></mapping>
        <mapping class="com.iago.springapp.domain.pojo.Images"></mapping>
        <mapping class="com.iago.springapp.domain.pojo.Items"></mapping>
        <mapping class="com.iago.springapp.domain.pojo.Notas"></mapping>
        <mapping class="com.iago.springapp.domain.pojo.NotasId"></mapping>
        <mapping class="com.iago.springapp.domain.pojo.Profile"></mapping>
        <mapping class="com.iago.springapp.domain.pojo.Shops"></mapping>
        <mapping class="com.iago.springapp.domain.pojo.Users"></mapping>
        
    </session-factory>
</hibernate-configuration>
